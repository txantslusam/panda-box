{
  "name": "panda-box",
  "version": "1.0.1",
  "description": "A lightbox plugin written in Typescript",
  "keywords": [
    "lightbox",
    "popup",
    "gallery"
  ],
  "main": "dist/pandaBox.js",
  "typings": "dist/types/main.d.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js  --progress --profile --color --display-error-details --display-cached --bail",
    "watch": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --inline --progress --watch --open",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/txantslusam/panda-box.git"
  },
  "author": "Mateusz Tomkowiak <matixnt1@windowslive.com>",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-advanced-variables": "^2.3.3",
    "postcss-atroot": "^0.1.3",
    "postcss-extend-rule": "^2.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-property-lookup": "^2.0.0",
    "postcss-url": "^8.0.0",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-webpack-plugin": "^1.2.2",
    "typescript": "^3.1.6",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.10"
  }
}
